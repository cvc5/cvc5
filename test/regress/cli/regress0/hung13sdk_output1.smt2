( set-logic QF_ALL)
( set-info :source | SMT-COMP'06 organizers |)
( set-info :smt-lib-version 2.6)
( set-info :category "check")
( set-info :status sat)
( declare-fun x1 () Bool)
( declare-fun x2 () Real)
( declare-fun x3 () Real)
( define-fun myRealIte ( ( a1 Bool) ( b1 Real) ( c1 Real)) Real
