; EXPECT: unsat
; COMMAND-LINE: --lang=sygus2 --cegqi-si=all --sygus-out=status
(set-logic LIA)
(define-fun letf ((z Int)) Int (+ z z))
(synth-fun f ((x Int) (y Int)) Int
    ((Start Int))
    ((Start Int (x
                 y
                 0
                 (- Start Start)
                 (letf Start)))))

(declare-var x Int)
(declare-var y Int)
(constraint (= (f x y) (* 3 x)))
(check-synth)
