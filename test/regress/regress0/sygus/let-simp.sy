; EXPECT: unsat
; COMMAND-LINE: --cegqi-si=all --sygus-out=status
(set-logic LIA)
(synth-fun f ((x Int) (y Int)) Int
    ((Start Int (x
                 y
                 0
                 (- Start Start)
                 (let ((z Int Start)) (+ z z))))))

(declare-var x Int)
(declare-var y Int)
(constraint (= (f x y) (* 3 x)))
(check-synth)

