; EXPECT: sat
(set-logic ALL)
(set-info :status sat)
(define-sort SetInt () (Bag Int))
(declare-fun A () (Bag Int))
(declare-fun B () (Bag Int))
(declare-fun x () Int)
(declare-fun y () Int)
(assert (>= (bag.count x (bag.union_disjoint A B)) 1))
(assert (not (>= (bag.count y A) 1)))
(assert (not (>= (bag.count y B) 1)))
(check-sat)
