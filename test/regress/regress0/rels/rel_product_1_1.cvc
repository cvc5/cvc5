% EXPECT: sat
OPTION "logic" "ALL";
IntPair: TYPE = [INT, INT, INT];
IntTup: TYPE = [INT, INT, INT, INT,INT, INT];
x : SET OF IntPair;
y : SET OF IntPair;
r : SET OF IntTup;

z : IntPair;
ASSERT z = (1,2,3);
zt : IntPair;
ASSERT zt = (3,2,1);


ASSERT z IS_IN x;
ASSERT zt IS_IN y;
ASSERT (1,1,1,1,1,1) IS_IN r;
ASSERT r = (x PRODUCT y);


CHECKSAT;
