% EXPECT: unsat
OPTION "logic" "ALL";
IntPair: TYPE = [INT, INT];
IntTup: TYPE = [INT, INT, INT, INT];
x : SET OF IntPair;
y : SET OF IntPair;
r : SET OF IntPair;

z : IntPair;
ASSERT z = (1,2);
zt : IntPair;
ASSERT zt = (2,1);
v : IntTup;
ASSERT v = (1,2,2,1);

ASSERT z IS_IN x;
ASSERT zt IS_IN y;
ASSERT NOT (v IS_IN (x PRODUCT y));

CHECKSAT;
