% EXPECT: sat
OPTION "logic" "ALL";
IntPair: TYPE = [INT, INT];
x : SET OF IntPair;
y : SET OF IntPair;
z : SET OF IntPair;
w : SET OF IntPair;

ASSERT z = TCLOSURE(x);
ASSERT w = (x JOIN y);
ASSERT (2, 2) IS_IN z;
ASSERT (0,3) IS_IN y;
ASSERT (-1,3) IS_IN y;
ASSERT (1,3) IS_IN y;
ASSERT (-2,3) IS_IN y;
ASSERT (2,3) IS_IN y;
ASSERT (3,3) IS_IN y;
ASSERT (4,3) IS_IN y;
ASSERT (5,3) IS_IN y;
ASSERT NOT (2, 3) IS_IN (x JOIN y);
ASSERT NOT (2,1) IS_IN x;

CHECKSAT;
