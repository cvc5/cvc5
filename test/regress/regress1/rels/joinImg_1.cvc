% EXPECT: unsat
OPTION "logic" "ALL";
OPTION "sets-ext";
Atom: TYPE;
x : SET OF [Atom, Atom];
y : SET OF [Atom, Atom];
r : SET OF [Atom, Atom];

t : SET OF [Atom];

a : Atom;
b : Atom;
c : Atom;
d : Atom;
e : Atom;

ASSERT TUPLE(a) IS_IN (x JOIN_IMAGE 2);
ASSERT x = {(b, c), (d, e), (c, e)};
ASSERT NOT(a = b);

CHECKSAT;
