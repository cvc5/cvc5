% EXPECT: sat
OPTION "logic" "ALL";
OPTION "sets-ext";
IntPair: TYPE = [INT, INT];
x : SET OF IntPair;
y : SET OF IntPair;
r : SET OF IntPair;

t : SET OF [INT];
u : SET OF [INT];

z : IntPair;
ASSERT z = (1,2);
zt : IntPair;
ASSERT zt = (2,1);
v : IntPair;
ASSERT v = (1,1);
a : IntPair;
ASSERT a = (1,5);
b: INT;

ASSERT (1, 7) IS_IN x;
ASSERT z IS_IN x;

ASSERT (7, 5) IS_IN y;

ASSERT t = (x JOIN_IMAGE 2);

ASSERT TUPLE(3) IS_IN (x JOIN_IMAGE 2);

ASSERT u = (x JOIN_IMAGE 1);

ASSERT TUPLE(4) IS_IN (x JOIN_IMAGE 2);

ASSERT TUPLE(b) IS_IN (x JOIN_IMAGE 1);
CHECKSAT;
