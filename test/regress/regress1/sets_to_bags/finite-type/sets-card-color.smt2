(set-logic ALL)
;(set-info :status unsat)
(set-option :produce-models true)
(set-option :sets-ext true)
(declare-datatype Color ((Red) (Green) (Blue)))
(declare-fun A () (Bag Color))
(declare-fun B () (Bag Color))
(assert (> (bag.card A) (bag.card B) ))
(assert (>= (bag.count Red B) 1))
(declare-fun d () Color)
(assert (not (= d Red)))
(assert (>= (bag.count d B) 1))
(assert (not (>= (bag.count Green A) 1)))
(check-sat)
