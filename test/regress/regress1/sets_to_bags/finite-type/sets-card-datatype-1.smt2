(set-logic ALL)
(set-info :status sat)
(set-option :produce-models true)
(set-option :sets-ext true)
(declare-datatype Rec ((mkRec (r1 (_ BitVec 2)) (r2 (_ BitVec 2)))))
(declare-fun A () (Bag Rec))
(declare-fun B () (Bag Rec))
(declare-fun universe () (Bag Rec))
(declare-fun x () Rec)
(assert (= (bag.card A) 5))
(assert (= (bag.card B) 5))
(assert (= (bag.card (bag.inter_min A B)) 1))
(assert (= universe (as set.universe (Bag Rec))))
(check-sat)
