(set-logic QF_ALL)
;(set-info :status unsat)
(set-option :produce-models true)
(set-option :sets-ext true)
(declare-fun A () (Bag (_ BitVec 2)))
(declare-fun B () (Bag (_ BitVec 2)))
(declare-fun C () (Bag (_ BitVec 2)))
(declare-fun D () (Bag (_ BitVec 2)))
(declare-fun x () (_ BitVec 2))
(declare-fun y () (_ BitVec 2))
(declare-fun z () (_ BitVec 2))

(assert (not (>= (bag.count x A) 1)))
(assert (not (>= (bag.count x B) 1)))
(assert (not (>= (bag.count x C) 1)))
(assert (not (>= (bag.count x D) 1)))
(assert (not (>= (bag.count y A) 1)))
(assert (not (>= (bag.count y B) 1)))
(assert (not (>= (bag.count y C) 1)))
(assert (not (>= (bag.count y D) 1)))
(assert (not (>= (bag.count z A) 1)))
(assert (not (>= (bag.count z B) 1)))
(assert (not (>= (bag.count z C) 1)))
(assert (not (>= (bag.count z D) 1)))

(assert (distinct x y z))

(assert (= (bag.card (bag.union_disjoint A (bag.union_disjoint B (bag.union_disjoint C D)))) 2))

(check-sat)
