(set-logic ALL)
(set-option :produce-models true)
(set-option :produce-unsat-cores true)
(set-option :incremental true)
(declare-const x Real)
(declare-const y Real)
(assert (< 0 x))
(assert (< 0 y))
(assert (< (+ x y) 1))
(assert (<= x y))
(check-sat)
(get-value (x y (- x y)))
(reset-assertions)
(declare-const a Int)
(declare-const b Int)
(assert (! (< 0 a) :named a0))
(assert (! (< 0 b) :named a1))
(assert (! (< (+ a b) 1) :named a2))
(assert (! (<= a b) :named a3))
(check-sat)
(get-unsat-core)
